# In this problem we have an list and we are finding the continuous addition of the elements that are present in the list
class Solution:
    def runningSum(self, nums: List[int]) -> List[int]:
        arr = []
        for i in range(len(nums)):
            total = 0
            for j in range(i + 1):
                total += nums[j]
            arr.append(total)
        return arr        
